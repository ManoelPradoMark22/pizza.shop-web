PAGINACAO:

    const paginatedOrders = filteredOrders.slice(
      pageIndex * 10,
      (pageIndex + 1) * 10,
    )

----------------------------

resto divisao (m√≥dulo operator % ):

const orderStatusArray = [
  'pending',
  'canceled',
  'processing',
  'delivering',
  'delivered',
  'newSTATUS',
] //length = 6

const DIVISOR = 6

for (let i = 0; i < 1000; i++) {
  const restoDaDivisao = i % DIVISOR
  console.log(`i % ${DIVISOR}`, ` | ${i} % ${DIVISOR}  =   ${restoDaDivisao}`)
  console.log(
    `orderStatusArray[${restoDaDivisao}]  =  ${orderStatusArray[restoDaDivisao]}`,
  )
  console.log('----------------')
}


-----------------

useCase:

const orders: IOrder[] = Array.from({ length: 60 }).map((_, i) => {
  return {
    orderId: `order-${i + 1}`,
    createdAt: new Date().toISOString(),
    status: orderStatusArray[i % 6], //ONLY STATUS AVAILABLE
    customerName: `customer-${i + 1}`,
    total: 24 * 100,
  }
})


//outupt:
i % 6  | 0 % 6  =   0
orderStatusArray[0]  =  pending
----------------
i % 6  | 1 % 6  =   1
orderStatusArray[1]  =  canceled
----------------
i % 6  | 2 % 6  =   2
orderStatusArray[2]  =  processing
----------------
i % 6  | 3 % 6  =   3
orderStatusArray[3]  =  delivering
----------------
i % 6  | 4 % 6  =   4
orderStatusArray[4]  =  delivered
----------------
i % 6  | 5 % 6  =   5
orderStatusArray[5]  =  newSTATUS
----------------
i % 6  | 6 % 6  =   0
orderStatusArray[0]  =  pending
----------------
